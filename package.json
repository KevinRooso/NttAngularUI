{
  "name": "NTT",
  "version": "0.0.1",
  "engines": {
    "node": "12.13.x",
    "npm": "6.12.x"
  },
  "scripts": {
    "test": "ng test",
    "e2e": "ng e2e",
    "lint:styles": "stylelint \"src/app/**/**/*.css\" && stylelint \"src/app/**/*.css\" && stylelint \"src/app/*.css\"",
    "lint": "ng lint --type-check --format=stylish && npm run lint:styles",
    "lint-ci": "ng lint --type-check --force --format=stylish && npm run lint:styles",
    "test-ci": "ng test --no-watch --no-progress --browsers=ChromeHeadlessNoSandbox",
    "e2e-ci": "ng e2e --protractor-config=e2e/protractor-ci.conf.js",
    "config": "ts-node --skip-project set-env.ts",
    "start:local": "npm run config && ng serve",
    "build:local": "npm run config && ng build",
    "heroku-postbuild": "npm run config && ng build -c=$NG_ENV"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~9.0.0",
    "@angular/cdk": "^9.1.0",
    "@angular/common": "~9.0.0",
    "@angular/compiler": "~9.0.0",
    "@angular/core": "~9.0.0",
    "@angular/forms": "~9.0.0",
    "@angular/localize": "^9.0.3",
    "@angular/material": "^9.1.0",
    "@angular/platform-browser": "~9.0.0",
    "@angular/platform-browser-dynamic": "~9.0.0",
    "@angular/router": "~9.0.0",
    "@ckeditor/ckeditor5-angular": "^1.2.3",
    "@ckeditor/ckeditor5-build-classic": "^18.0.0",
    "@ng-bootstrap/ng-bootstrap": "^6.0.0",
    "angular-datatables": "^9.0.1",
    "angular2-multiselect-dropdown": "^4.6.3",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "ckeditor4-angular": "^1.1.0",
    "compression": "^1.7.4",
    "datatables.net": "^1.10.20",
    "datatables.net-dt": "^1.10.20",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jquery": "^3.4.1",
    "ng-pick-datetime": "^7.0.0",
    "ng2-ckeditor": "^1.2.7",
    "rollbar": "^2.15.1",
    "rxjs": "~6.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.3",
    "@angular/cli": "~9.1.3",
    "@angular/compiler-cli": "~9.0.0",
    "@angular/language-service": "~9.0.0",
    "@types/datatables.net": "^1.10.18",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/jquery": "^3.3.33",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.1",
    "protractor": "~5.4.3",
    "puppeteer": "^2.1.1",
    "rxjs-tslint-rules": "^4.30.1",
    "source-map-explorer": "^2.4.2",
    "stylelint": "^13.3.2",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "tslint-angular": "^3.0.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.7.5"
  }
}
